//FizzBuzz
for (var i=1; i <= 100; i++)
{
    //checking if number is divisible by 3 and 5
    if (i % 3 == 0 && i % 5 == 0)
        console.log("FizzBuzz");
    //checking if number is divisible by 3
    else if (i % 3 == 0)
        console.log("Fizz");
    // checking if number is divisible by 5
    else if (i % 5 == 0)
        console.log("Buzz");
    //if not divisible just printing number
    else
        console.log(i);
}
